    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="506" d:DesignWidth="763">

    <Canvas x:Name="LayoutRoot" Height="506" Width="763" Background="Black">
        <ScrollViewer x:Name="scrollViewer" Height="391" Width="739" Background="Black" BorderThickness="0"  BorderBrush="Black" Canvas.Left="12" Canvas.Top="84" Style="{StaticResource ScrollViewerStyle1}"></ScrollViewer>
