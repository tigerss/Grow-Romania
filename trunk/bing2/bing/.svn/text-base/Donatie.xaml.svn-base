        <TextBlock Height="28" HorizontalAlignment="Left" Margin="504,18,0,0" Name="ValueTextBlock" VerticalAlignment="Top" Width="58" Foreground="#FF656464" Text="value:" FontSize="15" />
        <TextBlock Height="27" HorizontalAlignment="Left" Margin="557,16,0,0" Name="MoneyTextBlock" Text="bani" VerticalAlignment="Top" Width="166" Foreground="#FFAAA8A8" FontSize="18" />
        <Canvas Height="5" HorizontalAlignment="Left" Margin="12,79,0,0" Name="linieCanvas" VerticalAlignment="Top" />
        <!--<StackPanel HorizontalAlignment="Left" Height="0" Margin="12,84,0,0" Name="detaliiStackPanel" VerticalAlignment="Top" Width="664" />-->
        <StackPanel HorizontalAlignment="Left" Margin="30,0,0,0" Height="0" Name="detaliiComentariiStackPanel" VerticalAlignment="Top" Width="750" />
        <!--<Line Name="linieDonatii" X1="10" X2="630" Y1="86" Y2="86" Fill="White" ></Line>-->
    </Grid>
