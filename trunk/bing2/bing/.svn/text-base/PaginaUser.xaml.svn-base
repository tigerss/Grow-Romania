                <RectangleGeometry RadiusX="10" RadiusY="10" Rect="0,0,80,75">
                
                </RectangleGeometry>
            </Image.Clip>
        </Image>
        <Line X1="0" X2="205" Height="10" Width="205" StrokeThickness="1" Stroke="White" Canvas.Left="0" Canvas.Top="198"></Line>
