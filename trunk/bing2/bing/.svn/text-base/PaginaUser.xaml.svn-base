        <TextBlock Canvas.Left="8" Canvas.Top="404" Height="28" Text="Donations" Name="Donations" FontSize="13" Foreground="#69710b" Width="91">

        </TextBlock>
        <Line Canvas.Left="0" Canvas.Top="438" Height="18" Stroke="White" StrokeThickness="1" Width="205" X1="0" X2="205" />
        <TextBlock Canvas.Left="8" Canvas.Top="442" Height="58" Name="textBlock5" Width="91">
